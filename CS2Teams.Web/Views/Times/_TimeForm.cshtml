@model CS2Teams.Application.ViewModels.TimeViewModel

<div class="mb-3">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
    <span asp-validation-for="Nome" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="Tag"></label>
    <input asp-for="Tag" class="form-control" />
    <span asp-validation-for="Tag" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="Pais"></label>
    <input asp-for="Pais" class="form-control" />
    <span asp-validation-for="Pais" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="DataFundacao"></label>
    <input asp-for="DataFundacao" class="form-control" type="date" />
    <span asp-validation-for="DataFundacao" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="LogoUrl"></label>
    <input asp-for="LogoUrl" class="form-control" />
    <span asp-validation-for="LogoUrl" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="Ranking"></label>
    <input asp-for="Ranking" class="form-control" />
    <span asp-validation-for="Ranking" class="text-danger"></span>
</div>
